{
  "name": "fasthub/app",
  "type": "project",
  "description": "FastHub â€” Laravel 11 + PostgreSQL",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0",
    "ramsey/uuid": "^4.7",
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "pestphp/pest": "^2.33",
    "pestphp/pest-plugin-laravel": "^2.3",
    "nunomaduro/larastan": "^2.9",
    "phpstan/phpstan": "^1.10",
    "laravel/pint": "^1.16"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover --ansi || true"
    ],
    "test": "pest",
    "lint": [
      "pint"
    ],
    "stan": "phpstan analyse"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "platform": {
      "php": "8.2.0"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}

